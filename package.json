    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "css-loader": "0.23.1",
    "eslint": "2.13.1",
    "file-loader": "0.9.0",
    "font-awesome": "4.6.3",
    "jquery": "3.0.0",
    "jquery.scrollto": "2.1.2",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "raw-loader": "^0.5.1",
